#### import_parking.py ####

import csv
from django.core.management.base import BaseCommand
from testapp.models import ParkingLot  # ğŸ‘‰ ì•± ì´ë¦„ì— ë”°ë¼ ìˆ˜ì •í•˜ì„¸ìš”
from pathlib import Path

class Command(BaseCommand):
    help = 'Import parking data from CSV file'

    def add_arguments(self, parser):
        parser.add_argument('csv_file', type=str)

    def handle(self, *args, **options):
        csv_path = Path(options['csv_file'])
        if not csv_path.exists():
            self.stdout.write(self.style.ERROR('CSV íŒŒì¼ì´ ì¡´ì¬í•˜ì§€ ì•ŠìŠµë‹ˆë‹¤.'))
            return

        with open(csv_path, newline='', encoding='utf-8-sig') as csvfile:
            reader = csv.DictReader(csvfile)

            for row in reader:
                ParkingLot.objects.create(
                    number=int(row['ì£¼ì°¨ì¥ë²ˆí˜¸']),
                    name=row['ì£¼ì°¨ì¥ëª…'],
                    category=row['ì£¼ì°¨ì¥êµ¬ë¶„'],
                    type=row['ì£¼ì°¨ì¥ìœ í˜•'],
                    road_address=row['ì†Œì¬ì§€ë„ë¡œëª…ì£¼ì†Œ'] or None,
                    jibun_address=row['ì†Œì¬ì§€ì§€ë²ˆì£¼ì†Œ'] or None,
                    capacity=int(row['ì£¼ì°¨êµ¬íšìˆ˜']),
                    operating_days=row['ìš´ì˜ìš”ì¼'],
                    weekday_start=row['í‰ì¼ìš´ì˜ì‹œì‘ì‹œê°'],
                    weekday_end=row['í‰ì¼ìš´ì˜ì¢…ë£Œì‹œê°'],
                    saturday_start=row['í† ìš”ì¼ìš´ì˜ì‹œì‘ì‹œê°'],
                    saturday_end=row['í† ìš”ì¼ìš´ì˜ì¢…ë£Œì‹œê°'],
                    holiday_start=row['ê³µíœ´ì¼ìš´ì˜ì‹œì‘ì‹œê°'],
                    holiday_end=row['ê³µíœ´ì¼ìš´ì˜ì¢…ë£Œì‹œê°'],
                    fee_type=row['ìš”ê¸ˆì •ë³´'],
                    base_time=int(row['ì£¼ì°¨ê¸°ë³¸ì‹œê°„'] or 0),
                    base_fee=int(row['ì£¼ì°¨ê¸°ë³¸ìš”ê¸ˆ'] or 0),
                    unit_time=int(row['ì¶”ê°€ë‹¨ìœ„ì‹œê°„'] or 0),
                    unit_fee=int(row['ì¶”ê°€ë‹¨ìœ„ìš”ê¸ˆ'] or 0),
                    daily_pass_time=int(row['1ì¼ì£¼ì°¨ê¶Œìš”ê¸ˆì ìš©ì‹œê°„'] or 0) if row['1ì¼ì£¼ì°¨ê¶Œìš”ê¸ˆì ìš©ì‹œê°„'] else None,
                    daily_pass_fee=int(row['1ì¼ì£¼ì°¨ê¶Œìš”ê¸ˆ'] or 0) if row['1ì¼ì£¼ì°¨ê¶Œìš”ê¸ˆ'] else None,
                    monthly_pass_fee=int(row['ì›”ì •ê¸°ê¶Œìš”ê¸ˆ'] or 0) if row['ì›”ì •ê¸°ê¶Œìš”ê¸ˆ'] else None,
