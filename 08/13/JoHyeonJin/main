#### model.py ####

from django.db import models

class ParkingLot(models.Model):
    number = models.IntegerField()  # ì£¼ì°¨ì¥ë²ˆí˜¸
    name = models.CharField(max_length=100)  # ì£¼ì°¨ì¥ëª…
    category = models.CharField(max_length=20)  # ì£¼ì°¨ì¥êµ¬ë¶„ (ê³µì˜ ë“±)
    type = models.CharField(max_length=20)  # ì£¼ì°¨ì¥ìœ í˜• (ë…¸ì™¸, ë¶€ì„¤ ë“±)
    road_address = models.CharField(max_length=200, null=True, blank=True)  # ì†Œì¬ì§€ë„ë¡œëª…ì£¼ì†Œ
    jibun_address = models.CharField(max_length=200, null=True, blank=True)  # ì†Œì¬ì§€ì§€ë²ˆì£¼ì†Œ
    capacity = models.IntegerField()  # ì£¼ì°¨êµ¬íšìˆ˜

    operating_days = models.CharField(max_length=50)  # ìš´ì˜ìš”ì¼

    weekday_start = models.CharField(max_length=10)  # í‰ì¼ìš´ì˜ì‹œì‘ì‹œê°
    weekday_end = models.CharField(max_length=10)  # í‰ì¼ìš´ì˜ì¢…ë£Œì‹œê°
    saturday_start = models.CharField(max_length=10)  # í† ìš”ì¼ìš´ì˜ì‹œì‘ì‹œê°
    saturday_end = models.CharField(max_length=10)  # í† ìš”ì¼ìš´ì˜ì¢…ë£Œì‹œê°
    holiday_start = models.CharField(max_length=10)  # ê³µíœ´ì¼ìš´ì˜ì‹œì‘ì‹œê°
    holiday_end = models.CharField(max_length=10)  # ê³µíœ´ì¼ìš´ì˜ì¢…ë£Œì‹œê°

    fee_type = models.CharField(max_length=20)  # ìš”ê¸ˆì •ë³´ (ë¬´ë£Œ/ìœ ë£Œ/í˜¼í•© ë“±)
    base_time = models.IntegerField(default=0)  # ì£¼ì°¨ê¸°ë³¸ì‹œê°„ (ë¶„)
    base_fee = models.IntegerField(default=0)  # ì£¼ì°¨ê¸°ë³¸ìš”ê¸ˆ
    unit_time = models.IntegerField(default=0)  # ì¶”ê°€ë‹¨ìœ„ì‹œê°„
    unit_fee = models.IntegerField(default=0)  # ì¶”ê°€ë‹¨ìœ„ìš”ê¸ˆ

    daily_pass_time = models.IntegerField(null=True, blank=True)  # 1ì¼ì£¼ì°¨ê¶Œìš”ê¸ˆì ìš©ì‹œê°„
    daily_pass_fee = models.IntegerField(null=True, blank=True)  # 1ì¼ì£¼ì°¨ê¶Œìš”ê¸ˆ
    monthly_pass_fee = models.IntegerField(null=True, blank=True)  # ì›”ì •ê¸°ê¶Œìš”ê¸ˆ

    phone_number = models.CharField(max_length=20)  # ì „í™”ë²ˆí˜¸
    latitude = models.FloatField()  # ìœ„ë„
    longitude = models.FloatField()  # ê²½ë„

    is_disabled_parking = models.BooleanField()  # ì¥ì• ì¸ì „ìš©ì£¼ì°¨êµ¬ì—­ë³´ìœ ì—¬ë¶€

    def __str__(self):
        return f"{self.name} ({self.number})"

class TrafficCamera(models.Model):
    camera_id = models.CharField(max_length=20, unique=True)  # ê´€ë¦¬ë²ˆí˜¸
    address = models.CharField(max_length=255)                # ì£¼ì†Œ
    latitude = models.FloatField()                            # ìœ„ë„
    longitude = models.FloatField()                           # ê²½ë„

    def __str__(self):
        return f"{self.camera_id} - {self.address}"



#### views.py ####

from django.shortcuts import render
from django.http import JsonResponse
from .models import ParkingLot, TrafficCamera

# HTML í˜ì´ì§€ ë Œë”ë§
def main(request):
    return render(request, 'main.html')

def parking_lot_list(request):
    data = list(ParkingLot.objects.values('number', 'capacity', 'road_address', 'jibun_address', 'phone_number', 'name', 'latitude', 'longitude'))
    return JsonResponse(data, safe=False)

def traffic_camera_list(request):
    data = list(TrafficCamera.objects.values('camera_id', 'latitude', 'longitude'))
    return JsonResponse(data, safe=False)



